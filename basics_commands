copy <filename> <new location>
dir "\searchterm*" /s #search - /s - subdirectories
echo "text" #show text ; echo "text" > <filename> #create new file with "text"
runas #run cmd as another user - runas /user:localhost\administrator <command>
start #launch window to run specified command
systeminfo #show system info
tasklist #processes
type <filename> #show file contents
wevtutil #search log entries command line
wevtutil qe security f:/text | more
wmic #

-#SERVICE INFO#-
net start
sc query | more
tasklist /svc
